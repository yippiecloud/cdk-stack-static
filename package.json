{
  "name": "@yippiecloud/cdk-stack-static",
  "version": "1.1.0",
  "bin": {
    "cdk-stack-static": "bin/deploy.js"
  },
  "main": "lib/cdk-stack-static.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "example": "degit --force yippiecloud/template-website example && npm install --prefix example",
    "deploy": "npm run example && dotenv -- cdk deploy --all --require-approval never",
    "destroy": "dotenv -- cdk destroy"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.119.0",
    "@types/jest": "^27.0.1",
    "@types/adm-zip": "^0.4.34",
    "@types/node": "^16.7.10",
    "aws-cdk": "^1.119.0",
    "jest": "^27.1.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "degit": "^2.8.4",
    "dotenv-cli": "^4.0.0"
  },
  "dependencies": {
    "@aws-cdk/core": "^1.119.0",
    "@aws-cdk/aws-s3": "^1.119.0",
    "@aws-cdk/aws-s3-deployment": "^1.119.0",
    "@aws-cdk/aws-cloudfront": "^1.119.0",
    "@aws-cdk/aws-route53-targets": "^1.119.0",
    "@aws-cdk/aws-route53": "^1.119.0",
    "@aws-cdk/aws-certificatemanager": "^1.119.0",
    "@aws-cdk/aws-ssm": "^1.119.0",
    "@aws-sdk/client-s3": "^3.30.0",
    "adm-zip": "^0.5.5"
  }
}
